{
  "name": "@automagik/forge",
  "private": true,
  "version": "0.8.7-rc.28",
  "type": "module",
  "scripts": {
    "dev": "cross-env VITE_OPEN=${VITE_OPEN:-false} vite",
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=4096 tsc && cross-env NODE_OPTIONS=--max-old-space-size=4096 vite build",
    "check": "tsc --noEmit",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 125",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "lint:i18n": "cross-env LINT_I18N=true eslint . --ext ts,tsx --max-warnings 0",
    "lint:dead-code": "knip",
    "lint:dead-code:prod": "knip --production --cache",
    "lint:dead-code:strict": "knip --production --strict --cache",
    "lint:dead-code:fix": "knip --fix",
    "lint:dead-code:trace": "knip --trace",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "cap:sync": "npx cap sync",
    "cap:open:android": "npx cap open android",
    "cap:run:android": "npx cap run android",
    "cap:build:android": "npm run build && npx cap sync && cd ../android && ./gradlew assembleDebug"
  },
