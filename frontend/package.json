{
  "name": "@automagik/forge",
  "private": true,
  "version": "0.7.2",
  "type": "module",
  "scripts": {
    "dev": "cross-env VITE_OPEN=${VITE_OPEN:-false} vite",
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=4096 tsc && cross-env NODE_OPTIONS=--max-old-space-size=4096 vite build",
    "check": "tsc --noEmit",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 110",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "lint:i18n": "cross-env LINT_I18N=true eslint . --ext ts,tsx --max-warnings 0",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "cap:sync": "npx cap sync",
    "cap:open:android": "npx cap open android",
    "cap:run:android": "npx cap run android",
    "cap:build:android": "npm run build && npx cap sync && cd ../android && ./gradlew assembleDebug",
    "perf:bundle": "tsx performance-tests/bundle-size.test.ts",
    "perf:lighthouse": "tsx performance-tests/run-server-tests.ts lighthouse",
    "perf:load-time": "tsx performance-tests/run-server-tests.ts load-time",
    "perf:animation": "tsx performance-tests/run-server-tests.ts animation-fps",
    "perf:server-tests": "tsx performance-tests/run-server-tests.ts",
    "perf:all": "npm run perf:bundle && npm run perf:server-tests",
    "perf:baseline": "npm run perf:bundle"
  },
